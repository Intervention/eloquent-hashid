{
    "name": "intervention/eloquent-hashid",
    "description": "Configurable HashId Trait for Laravel Eloquent models",
    "homepage": "http://intervention.io/",
    "keywords": ["laravel", "eloquent", "hashid"],
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Vogel",
            "email": "oliver@intervention.io",
            "homepage": "https://intervention.io/"
        }
    ],
    "require": {
        "php": "^8.0",
        "hashids/hashids": "^5"
    },
    "autoload": {
        "psr-4": {
            "Intervention\\EloquentHashid\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Intervention\\EloquentHashid\\Laravel\\EloquentHashidServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
